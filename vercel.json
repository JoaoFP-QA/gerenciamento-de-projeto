{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "backend.js" },
      { "src": "/(.*)", "dest": "/" }
    ],
    "functions": {
      "backend.js": {
        "runtime": "nodejs12.x",
        "memory": 1024
      }
    },
    "env": {
      "PORT": 5000
    },
    "scripts": {
      "build": "npm run build",
      "backend": "npm run backend"
    }
  }
  